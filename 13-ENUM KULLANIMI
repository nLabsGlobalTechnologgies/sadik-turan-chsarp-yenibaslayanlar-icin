using System;

namespace _1_SadikTuran_temelCEgitimi
{
    internal partial class Program
    {
        enum MyEnum
        {
            Adana=1,
            Mersin=2,
        }

        static void Main(string[] args)
        {
            var ilkEnum = MyEnum.Adana;
            var sonrakiEnum = MyEnum.Mersin;
            if (ilkEnum==1)
            {
                Console.WriteLine(ilkEnum.ToString() + "Adana Plakası Dogru Bir Karar");
            }
            else
            {
                Console.WriteLine("else işlemi degil yani");
            }


            Console.ReadLine();
        }        
    }
}
