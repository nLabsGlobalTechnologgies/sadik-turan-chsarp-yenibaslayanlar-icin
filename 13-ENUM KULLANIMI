using System;

namespace _1_SadikTuran_temelCEgitimi
{
    internal partial class Program
    {
        enum MyEnum
        {
            Adana=1,
            Mersin=2
        }

        static void Main(string[] args)
        {
            var ilkEnum = MyEnum.Adana;
            var sonrakiEnum = MyEnum.Mersin;
            var sehirId = (int)ilkEnum;
            if (ilkEnum<sonrakiEnum)
            {
                Console.WriteLine(sehirId + " Nolu Şehirdesiniz");
                Console.WriteLine(ilkEnum.ToString());
            }
            else
            {
                Console.WriteLine("else işlemi degil yani");
            }


            Console.ReadLine();
        }        
    }
}
