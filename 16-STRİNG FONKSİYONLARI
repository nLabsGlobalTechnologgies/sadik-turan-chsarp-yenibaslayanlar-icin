//String İşlemleri
//1. Equals Kullanımı / if (str1.Equals(str2) == true)
//2. Compare Kullanımı / if (string.Compare(str1, str2) == 0)
//3.
//
string _email = "abc@abc.com";
string _password = "abc";

Console.WriteLine("Email Adresi Girin");
string email = Console.ReadLine();
Console.WriteLine("Parola Girin");
string password = Console.ReadLine();
if (string.Compare(email, _email)==0 && string.Compare(password, _password)== 0)
//if (_email.Equals(email) && _password.Equals(password))
{
    Console.WriteLine("Bilgiler Dogru Hoş geldin");
}
else
{
    Console.WriteLine("Lütfen tekrar deneyiniz");
}

Console.ReadLine();

